
public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s1 = new String("Hamada yel3ab");
		mystery(s1);
	}
	public static int mystery(String s)
	{
	    char[] letters = s.toCharArray();
	    int x = 0;
	    for (int i = 0; i < letters.length; i++) {
	        if (letters[i] == ' ') {
	           letters[i] = '_';
	           x++;
	        }
	    }
	    return x;
	}
}
